local function createSocialBtn(text, color, pos, callback)
    local btn = Instance.new("TextButton")
    btn.Text = text
    btn.Size = UDim2.new(0.9, 0, 0, 35)
    btn.Position = pos
    btn.BackgroundColor3 = color
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.Parent = menuFrame
    Instance.new("UICorner", btn)
    if color == Color3.fromRGB(0,0,0) then
        local stroke = Instance.new("UIStroke")
        stroke.Color = Color3.fromRGB(255,255,255)
        stroke.Thickness = 1
        stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        stroke.Parent = btn
    end
    btn.MouseButton1Click:Connect(callback)
end

local minIcon = Instance.new("TextButton")
minIcon.Name = "MinimizedIcon"
minIcon.Size = UDim2.new(0, 50, 0, 50)
minIcon.Position = UDim2.new(0.9, -60, 0.1, 0)
minIcon.BackgroundColor3 = Color3.new(0, 0, 0)
minIcon.Text = "S"
minIcon.TextColor3 = Color3.fromRGB(255, 0, 0)
minIcon.TextSize = 30
minIcon.Font = Enum.Font.GothamBold
minIcon.Visible = false
minIcon.Parent = screenGui
Instance.new("UICorner", minIcon).CornerRadius = UDim.new(1, 0)
Instance.new("UIStroke", minIcon).Color = Color3.fromRGB(50, 50, 50)

local taps = 0
local isLocked = false

local function openSocial(url)
    setclipboard(url)
    pcall(function() GuiService:OpenBrowserWindow(url) end)
end

local function activateAmoled()
    menuFrame.Visible = false
    minIcon.Visible = false
    overlay.Visible = true
    overlay.BackgroundTransparency = 1
    taps = 0
    isLocked = true
    countdownLabel.Visible = true
    for i = 3, 1, -1 do
        countdownLabel.Text = tostring(i)
        task.wait(1)
    end
    countdownLabel.Text = ""
    countdownLabel.Visible = false
    local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
    local tween = TweenService:Create(overlay, tweenInfo, {BackgroundTransparency = 0})
    tween:Play()
end

local function deactivateAmoled()
    local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
    local tween = TweenService:Create(overlay, tweenInfo, {BackgroundTransparency = 1})
    tween:Play()
    tween.Completed:Connect(function()
        overlay.Visible = false
        isLocked = false
        taps = 0
        menuFrame.Visible = true
    end)
end

activateBtn.MouseButton1Click:Connect(activateAmoled)

unlockButton.MouseButton1Click:Connect(function()
    if isLocked then
        taps = taps + 1
        if taps >= 2 then deactivateAmoled() end
    end
end)

createSocialBtn("TikTok", Color3.fromRGB(0, 0, 0), UDim2.new(0.05, 0, 0.38, 0), function()
    openSocial(LinkTikTok)
end)

createSocialBtn("Ir Para Youtube", Color3.fromRGB(255, 0, 0), UDim2.new(0.05, 0, 0.53, 0), function()
    openSocial(LinkYoutube)
end)

createSocialBtn("Ir Para O Instagram", Color3.fromRGB(193, 53, 132), UDim2.new(0.05, 0, 0.68, 0), function()
    openSocial(LinkInstagram)
end)

minBtn.MouseButton1Click:Connect(function()
    menuFrame.Visible = false
    minIcon.Visible = true
end)

minIcon.MouseButton1Click:Connect(function()
    minIcon.Visible = false
    menuFrame.Visible = true
end)

closeBtn.MouseButton1Click:Connect(function() screenGui:Destroy() end)

task.spawn(activateAmoled)
